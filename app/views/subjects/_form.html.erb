<%= error_messages_for(f.object) %>

<table summary="Subject form fields">
	<tr>
		<td>Name</td>
		<td><%= f.text_field(:name) %></td>
	</tr>
	<tr>
		<td>Position</td>
		<td><%= f.select(:position, 1..@subject_count) %></td>
	</tr>
	<tr>
		<td>Visible</td>
		<td><%= f.check_box(:visible) %></td>
	</tr>
	<tr>
		<th>Created at</th>
		<td>
			<%= f.datetime_select(:created_at) %>
		</td>
	</tr>
</table> 